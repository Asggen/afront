{
  "name": "afront",
  "version": "1.0.9",
  "description": "AFront is a front-end JavaScript library designed to create seamless server-side rendered (SSSR) websites.",
  "main": "webpack.dev.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:ssr": "webpack --config webpack.ssr.prod.js",
    "build:alt": "webpack --config webpack.build-prod.js",
    "prod:ssr": "node build-prod-ssr/ssr.prod.js",
    "start": "webpack serve --config webpack.dev.js",
    "static": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Asggen/afront"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "afront",
    "ssr",
    "web"
  ],
  "author": "imarksea - Piyush Nishad",
  "files": [
    "install.js",
    "server.js",
    "webpack.build-prod.js",
    "webpack.dev.js",
    "webpack.prod.js",
    "webpack.ssr.prod.js",
    ".babelrc",
    ".env",
    "src/**/*",
    "build-prod/**/*",
    "build-prod-static/**/*",
    "build-prod-ssr/**/*"
  ],
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.15",
    "afront": "^1.0.5",
    "asggen-headtags": "^1.0.3",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.4.0",
    "extract-zip": "^2.0.0",
    "follow-redirects": "^1.15.6",
    "fs-extra": "^11.2.0",
    "ignore-styles": "^5.0.1",
    "json5": "^1.0.2",
    "loader-utils": "^1.4.2",
    "react-router-dom": "^6.26.0",
    "readline-sync": "^1.4.10",
    "styled-components": "^6.1.12",
    "terser-webpack-plugin": "^5.3.10",
    "postcss": "^8.4.31",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/register": "^7.24.6",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "express": "^4.19.2",
    "html-webpack-plugin": "^5.6.0",
    "ignore-styles": "^5.0.1",
    "json5": "^1.0.2",
    "mini-css-extract-plugin": "^2.9.0",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0",
    "postcss": "^8.4.31"
  },
  "resolutions": {
    "loader-utils": "^1.4.2"
  },
  "bin": {
    "afront": "install.js"
  }
}
